"undefined"==typeof CYZS&&(CS=CYZS={}),"undefined"==typeof CYZS&&(CS=CYZS={}),CYZS.PanelMenu={init:function(){var n=this,e={method:"Userinfo.get"};CYZS.AjaxHelper.ajax({url:CYZS.Config.URL_PROXY+"m.php",data:e,onSuccess:function(e){$("#js_mobile_aside_nav_container").html(template("mobileAsideNavTpl",e.data)),n.toggleShow()}})},toggleShow:function(){var n=this;$("#clickPanelMenu").on("click",function(){n.showPanel()}),$("#menuMask").on("click",function(){n.hidePanel()}),$("#menuMask").length&&($("#menuMask").get(0).ontouchstart=function(){n.hidePanel()})},showPanel:function(){$("#menuPanel").css({right:-250}),$("#menuPanel").animate({right:"0"}),$("#menuMask").show()},hidePanel:function(){$(".page").hasClass("pageActive")?$("#menuPanel").animate({right:"-12.41rem"}):$("#menuPanel").animate({right:"-250"}),$("#menuMask").hide()}},document.ready=function(){CYZS.PanelMenu.init()};